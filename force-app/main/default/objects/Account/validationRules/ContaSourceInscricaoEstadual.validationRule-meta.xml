<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContaSourceInscricaoEstadual</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
$RecordType.Name = &quot;Conta&quot;,
ISBLANK(InscricaoEstadual__c)
)</errorConditionFormula>
    <errorDisplayField>InscricaoEstadual__c</errorDisplayField>
    <errorMessage>Encontramos um seguinte obstáculo, Revise os seguintes campos: Inscrição Estadual</errorMessage>
</ValidationRule>
